#include<stdio.h>
int main()
{
    int n1,n2,n3,large;
    printf("Enter the first number",n1);
    scanf("%d",&n1);
    printf("Enter the second number",n2);
    scanf("%d",&n2);
    printf("Enter the third number",n3);
    scanf("%d",&n3);
    large=n1>n2?(n1>n3?n1:n3):(n2>n3?n2:n3);
    printf("\n The Largest number is:%d",large);
    return 0;
}
