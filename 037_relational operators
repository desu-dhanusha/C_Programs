#include<stdio.h>
#include<math.h>
int main()
{
  int a=54,b=72;
  printf("\n %d < %d = %d",a,b,a<b);
  printf("\n %d == %d = %d",a,b,a==b);
  printf("\n %d != %d = %d",a,b,a!=b);
  printf("\n %d > %d = %d",a,b,a>b);
  printf("\n %d >= %d = %d",a,b,a>=b);
  printf("\n %d <= %d = %d",a,b,a<=b);
  return 0;
}
