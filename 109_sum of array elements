#include <stdio.h>
int getSum(int arr[], int n)
{
    int sum = 0;
    int i;
    for ( i = 0; i < n; i++)
    {
       sum += arr[i];
    }
    return sum;
}

int main() 
{
    int arr[] = {1, 2, 3, 4, 5};
    int n = sizeof(arr) / sizeof(arr[0]);
    int res = getSum(arr, n);
  
    printf("%d", res);
    return 0;
}
