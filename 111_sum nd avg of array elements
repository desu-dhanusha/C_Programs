#include <stdio.h>
int main()
{
    int arr[] = {10, 20, 30, 40, 50}; 
    int n = sizeof(arr) / sizeof(arr[0]); 
    int sum = 0;
    double average;
for (int i = 0; i < n; i++)
  {
        sum += arr[i];
  }
    average = (double)sum / n; 

    printf("Sum of array elements: %d\n", sum);
    printf("Average of array elements: %.2f\n", average); 
return 0;
}
