#include<stdio.h>
int main() 
{
    int num=6;
    float amt=8.79;
    char code= 'S';
    double pi=3.14567899043;
    long int population_of_India=10000000000;
    char msg[]="Hello";
    printf("\n NUM=%d \n AMT=%f \n CODE=%c \n PI=%e \n POPULATION OF INDIA=%ld \n  MESSAGE=%s",num,amt,code,pi,population_of_India, msg);
    return 0;
}
