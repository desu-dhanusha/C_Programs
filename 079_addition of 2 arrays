#include <stdio.h>
int main() {
    int n, i;
    printf("Enter number of elements: ");
    scanf("%d", &n);

    int arr1[n], arr2[n], sum[n];

    printf("Enter elements of first array:\n");
    for(i = 0; i < n; i++)
        scanf("%d", &arr1[i]);

    printf("Enter elements of second array:\n");
    for(i = 0; i < n; i++)
        scanf("%d", &arr2[i]);

    // Add arrays
    for(i = 0; i < n; i++)
        sum[i] = arr1[i] + arr2[i];

    printf("Sum of the arrays: ");
    for(i = 0; i < n; i++)
        printf("%d ", sum[i]);

    return 0;
}
